public class Truck extends Vehicle{
   
    // No-args constructor -> Create a default truck.
    public Truck(String licensePlate, double hours, Boolean isHandicap, Boolean isTruck, Boolean isRegular){
        super(licensePlate, hours, isHandicap, isTruck, isRegular); // Call parent Vehicle.
    }
    
}
